<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane alignment="center" hgap="10" minWidth="400.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Label text="Nama" />
      <Label text="Kode" GridPane.rowIndex="1" />
      <TextField fx:id="tfName" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" />
      <TextField fx:id="tfCode" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Button cancelButton="true" mnemonicParsing="false" onAction="#onCloseSchoolEditPressed" text="Tutup" GridPane.rowIndex="2" />
      <HBox GridPane.columnIndex="1" GridPane.rowIndex="2">
         <children>
            <Pane HBox.hgrow="ALWAYS" />
            <Button defaultButton="true" mnemonicParsing="false" onAction="#onSaveSchoolEditPressed" text="Simpan" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>
