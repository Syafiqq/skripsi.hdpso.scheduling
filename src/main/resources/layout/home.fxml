<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="home_container" minHeight="600.0" minWidth="800.0"
            xmlns="http://javafx.com/javafx/8.0.65">
    <top>
        <BorderPane fx:id="home_ribbon" BorderPane.alignment="CENTER">
            <top>
                <HBox spacing="20.0" style="-fx-background-color: #E0E0E0" BorderPane.alignment="CENTER">
                    <BorderPane.margin>
                        <Insets/>
                    </BorderPane.margin>
                    <children>
                        <Button fx:id="home_menu_home" mnemonicParsing="false" onAction="#onHomeClick" text="Utama"/>
                        <Button fx:id="home_menu_configuration" mnemonicParsing="false" onAction="#onConfigurationClick"
                                text="Konfigurasi"/>
                        <Button fx:id="home_menu_development" mnemonicParsing="false" onAction="#onDevelopmentClick"
                                text="Development"/>
                    </children>
                    <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </padding>
                </HBox>
            </top>
            <BorderPane.margin>
                <Insets bottom="10.0"/>
            </BorderPane.margin>
        </BorderPane>
    </top>
</BorderPane>
