<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<HBox xmlns:fx="http://javafx.com/fxml/1" spacing="5.0"
      style="-fx-background-color: #c3c4c4,    linear-gradient(#d6d6d6 50%, white 100%),    radial-gradient(center 50% -40%, radius 200%, #e6e6e6 45%, rgba(230,230,230,0) 50%); -fx-effect: dropshadow( three-pass-box , rgba(0,0,0,0.6) , 3, 0.0 , 0 , 1 );;"
      xmlns="http://javafx.com/javafx/8.0.65">
    <children>
        <Button alignment="CENTER" contentDisplay="TOP" minWidth="80.0" mnemonicParsing="false" text="Baru">
            <graphic>
                <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../drawable/document_add_256.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button alignment="CENTER" contentDisplay="TOP" minWidth="80.0" mnemonicParsing="false" text="Buka">
            <graphic>
                <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../drawable/open_48.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Pane HBox.hgrow="ALWAYS"/>
        <Button alignment="CENTER" contentDisplay="TOP" minWidth="80.0" mnemonicParsing="false" text="Keluar">
            <graphic>
                <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../drawable/exit.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
</HBox>
