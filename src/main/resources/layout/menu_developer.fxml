<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<HBox xmlns:fx="http://javafx.com/fxml/1" spacing="5.0"
      style="-fx-background-color: #c3c4c4,    linear-gradient(#d6d6d6 50%, white 100%),    radial-gradient(center 50% -40%, radius 200%, #e6e6e6 45%, rgba(230,230,230,0) 50%); -fx-effect: dropshadow( three-pass-box , rgba(0,0,0,0.6) , 3, 0.0 , 0 , 1 );;"
      xmlns="http://javafx.com/javafx/8.0.65">
    <children>
        <Button alignment="CENTER" contentDisplay="TOP" minWidth="125.0" mnemonicParsing="false"
                text="Konfigurasi Metode" wrapText="true">
            <graphic>
                <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../drawable/parameters_32.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button alignment="CENTER" contentDisplay="TOP" minWidth="125.0" mnemonicParsing="false"
                text="Konfigurasi Constraint">
            <graphic>
                <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../drawable/data_filter_128.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button alignment="CENTER" contentDisplay="TOP" minWidth="125.0" mnemonicParsing="false" text="Export"
                visible="false">
            <graphic>
                <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../drawable/export_32.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
</HBox>
