<!--suppress JavaFxDefaultTag -->
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane alignment="center" hgap="20.0" vgap="20.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
    <children>
        <Label text="Nama" />
        <Label text="Nama Pendek" GridPane.rowIndex="1" />
        <Label text="Alamat" GridPane.rowIndex="2" />
        <Label text="Tahun Ajaran" GridPane.rowIndex="3" />
        <Label text="Semester" GridPane.rowIndex="4" />
        <Label text="Jumlah Hari Aktif" GridPane.rowIndex="5" />
        <Label text="Jumlah Periode Aktif" GridPane.rowIndex="6" />
        <HBox minWidth="300.0" GridPane.columnIndex="1" GridPane.rowIndex="7">
            <children>
                <Pane HBox.hgrow="ALWAYS" />
                <Button defaultButton="true" mnemonicParsing="false" onAction="#onSchoolDetailEditPressed" text="Edit" />
            </children>
        </HBox>
        <Button cancelButton="true" mnemonicParsing="false" onAction="#onSchoolDetailClosePressed" text="Tutup" GridPane.rowIndex="7" />
      <Label fx:id="lName" text="Label" GridPane.columnIndex="1" />
      <Label fx:id="lNickname" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Label fx:id="lAddress" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
             <Label fx:id="lAcademicYear" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3" />
             <Label fx:id="lSemester" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="4" />
             <HBox alignment="CENTER" spacing="20.0" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="5">
                 <children>
                     <Label fx:id="lActiveDay" alignment="CENTER" contentDisplay="CENTER" text="Label" />
            <Pane HBox.hgrow="ALWAYS" />
            <Button mnemonicParsing="false" onAction="#onSchoolDayEditPressed" text="Edit Hari" />
         </children>
      </HBox>
        <HBox alignment="CENTER" spacing="20.0" GridPane.columnIndex="1" GridPane.rowIndex="6">
            <children>
                <Label fx:id="lActivePeriod" text="Label" />
                <Pane HBox.hgrow="ALWAYS" />
                <Button mnemonicParsing="false" onAction="#onSchoolPeriodEditPressed" text="Edit Periode" />
            </children>
      </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</GridPane>
