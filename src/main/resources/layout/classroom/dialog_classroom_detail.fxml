<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane alignment="center" hgap="10" maxWidth="700.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
    <children>
        <Label text="Nama" />
        <Label fx:id="lName" text="Label" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" />
        <Label text="Timeoff" GridPane.rowIndex="1" />
        <Button cancelButton="true" mnemonicParsing="false" onAction="#onCloseClassroomDetailPressed" text="Tutup" GridPane.rowIndex="2" />
        <HBox spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
            <children>
                <Pane HBox.hgrow="ALWAYS" />
                <Button mnemonicParsing="false" onAction="#onTimeOffEditClassroomDetailPressed" text="Edit Timeoff" />
                <Button defaultButton="true" mnemonicParsing="false" onAction="#onEditClassroomDetailPressed" text="Edit Kelas" />
            </children>
        </HBox>
        <GridPane fx:id="timeOffContainer" hgap="8.0" vgap="8.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1">
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
        </GridPane>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</GridPane>